{
  "name": "miaoxing/wechat-store",
  "description": "Miaoxing Wechat Store Plugin",
  "keywords": [
    "miaoxing", "wechat", "store"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "dev-master@dev",
    "miaoxing/shop": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\WechatStore\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\WechatStore\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
